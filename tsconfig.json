{
  "extends": "astro/tsconfigs/strict",
  
  "compilerOptions": {
    "strictNullChecks": true,
    "noImplicitAny": false,
    "strict": false,
    "baseUrl": ".",
    "paths": {
      "@components/*": [
        "src/components/*"
      ],
      "@layouts/*": [
        "src/layouts/*"
      ]
    }
  }
}